    <script type="text/javascript" src="~/Content/Admin/js/timeago/jquery.timeago.min.js"></script>
    <!-- FULL CALENDAR -->
    <script type="text/javascript" src="~/Content/Admin/js/fullcalendar/fullcalendar.min.js"></script>
