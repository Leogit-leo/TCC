                            <li><a class="" href="@Url.Action("IndexAgenda","Agenda")"><span class="sub-menu-text">Agendas Múltiplas</span></a></li>
                        </ul>
                    </li>

                    <li class="has-sub">
                        <a href="javascript:;" class="">
                            <i class="fa fa-book fa-fw"></i> <span class="menu-text">Gerenciar Compra</span>
                            <span class="arrow"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="@Url.Action("Index", "Compra")"><span class="sub-menu-text">Compra de Produto</span></a></li>
                            <li><a class="" href="@Url.Action("Index", "CompraFiado")"><span class="sub-menu-text"> Serviço / Produto Fiado</span></a></li>
                           
                        </ul>
                    </li>

                    <li class="has-sub">
                        <a href="javascript:;" class="">
                            <i class="fa fa-home fa-fw"></i> <span class="menu-text">Meu Salão</span>
                            <span class="arrow"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="@Url.Action("Index","CategoriaFoto")"><span class="sub-menu-text">Categoria de Fotos</span></a></li>
                            <li><a class="" href="@Url.Action("Index","Foto")"><span class="sub-menu-text">Enviar Fotos</span></a></li>
                        </ul>
                    </li>
                 
                    
                </ul>
                <!-- /SIDEBAR MENU -->
            </div>
        </div>
        <!-- /SIDEBAR -->
        <div id="main-content">
            @RenderBody()
        </div>
</section>
