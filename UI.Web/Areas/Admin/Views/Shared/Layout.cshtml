                        <i class="fa fa-angle-down"></i>
                    </a>
                    <ul class="dropdown-menu">
                        <li><a href="@Url.Action("MeuPerfil","Home", new { id = usuario.FuncionarioId})"><i class="fa fa-user"></i> Meu Perfil</a></li>
                        <li><a href="@Url.Action("Logout","Login")"><i class="fa fa-power-off"></i> Log Out</a></li>
                    </ul>
                </li>
                <!-- END USER LOGIN DROPDOWN -->
            </ul>
            <!-- END TOP NAVIGATION MENU -->
        </div>
     
